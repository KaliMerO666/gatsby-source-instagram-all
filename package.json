{
  "name": "gatsby-source-instagram-all",
  "description": "Gatsby source plugin for fetching all instagram posts",
  "version": "4.2.0",
  "author": "Mantas Mikalauskas <mantas.codes@gmail.com>",
  "bugs": {
    "url": "https://github.com/MantasMikal/gatsby-source-instagram-all"
  },
  "homepage": "https://github.com/MantasMikal/gatsby-source-instagram-all",
  "main": "gatsby-node.js",
  "license": "MIT",
  "scripts": {
    "build": "babel src --out-dir dist --ignore **/__tests__",
    "watch": "babel --watch src --out-dir dist --ignore **/__tests__",
    "format": "prettier --write src/**/*.js",
    "clean": "rm -rf dist",
    "prepublishOnly": "yarn run build",
    "prepare": "yarn run build"
  },
  "dependencies": {
    "gatsby-source-filesystem": "^3.0.0",
    "node-fetch": "^2.6.1",
    "query-string": "^7.0.0"
  },
  "peerDependencies": {
    "gatsby": ">3.0.0-alpha"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.14.0",
    "@babel/node": "^7.13.13",
    "@babel/preset-env": "^7.14.1",
    "prettier": "^2.3.0"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "source",
    "instagram",
    "api",
    "all",
    "post",
    "fetch"
  ]
}
